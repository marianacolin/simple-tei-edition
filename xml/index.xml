         <titleStmt>
            <title>Edition of Dickinson Poem "The Sea of Sunset"</title>
         </titleStmt>
         <publicationStmt>
            <p>Publication information</p>
         </publicationStmt>
         <sourceDesc>
            <p>Information about the source</p>
         </sourceDesc>
